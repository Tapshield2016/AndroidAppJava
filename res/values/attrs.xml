<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="PageIndicator">
        <attr name="color" format="reference|color"/>
        <attr name="strokePercentOfDiameter" format="reference|float"/>
    </declare-styleable>
    
    <declare-styleable name="StepIndicator">
        <attr name="colorIncomplete" format="reference|color"/>
        <attr name="colorComplete" format="reference|color"/>
    </declare-styleable>
    
    <declare-styleable name="CircleButton">
        <attr name="text" format="reference|string"/>
        <attr name="textColor" format="reference|color"/>
        <attr name="textSize" format="reference|dimension"/>
        <attr name="background" format="reference|integer"/>
        <attr name="icon" format="reference|integer"/>
        <attr name="iconPadding" format="reference|dimension"/>
        <attr name="iconPaddingLeft" format="reference|dimension"/>
        <attr name="iconPaddingTop" format="reference|dimension"/>
        <attr name="iconPaddingRight" format="reference|dimension"/>
        <attr name="iconPaddingBottom" format="reference|dimension"/>
    </declare-styleable>
    
    <declare-styleable name="CircleSeekBar">
        <attr name="colorBar" format="reference|color"/>
        <attr name="colorHandle" format="reference|color"/>
        <attr name="colorProgress" format="reference|color"/>
        <attr name="sizeBar" format="reference|dimension"/>
        <attr name="sizeHandle" format="reference|dimension"/>
        <attr name="sizeProgress" format="reference|dimension"/>
    </declare-styleable>
    
</resources>
