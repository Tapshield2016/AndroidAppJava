<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="PageIndicator">
        <attr name="color" format="reference|color"/>
        <attr name="strokePercentOfDiameter" format="reference|float"/>
    </declare-styleable>
    
    <declare-styleable name="StepIndicator">
        <attr name="colorIncomplete" format="reference|color"/>
        <attr name="colorComplete" format="reference|color"/>
    </declare-styleable>
    
    <declare-styleable name="CircleButton">
        <attr name="text" format="reference|string"/>
        <attr name="textColor" format="reference|color"/>
        <attr name="textSize" format="reference|dimension"/>
        <attr name="background" format="reference|integer"/>
        <attr name="icon" format="reference|integer"/>
    </declare-styleable>
    
</resources>
