<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    
    <ListView
        android:id="@+id/fragment_organizationselection_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    
    <TextView
        android:id="@+id/fragment_organizationselection_empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="@string/ts_organizationselection_list_empty"
        android:textColor="@color/ts_gray_dark"
        android:textSize="@dimen/ts_common_text_m"
        android:visibility="gone"/>
</FrameLayout>
